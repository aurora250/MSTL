@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(@MSTL_ENABLE_BOOST@)
    find_dependency(Boost COMPONENTS system filesystem)
endif()

if(@MSTL_ENABLE_QT6@)
    find_dependency(Qt6 COMPONENTS Core Widgets Gui)
endif()

if(@MSTL_ENABLE_CUDA@)
    find_dependency(CUDAToolkit)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/MSTLTargets.cmake")
check_required_components(MSTL)

set(MSTL_INCLUDE_DIR "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
message(STATUS "Found MSTL: ${CMAKE_CURRENT_LIST_DIR} (include: ${MSTL_INCLUDE_DIR})")
